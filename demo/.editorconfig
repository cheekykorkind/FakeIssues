; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_style = space
indent_size = 4

[*.yml]
indent_style = space
indent_size = 2

[*.jsp]
indent_style = space
indent_size = 2

[*.css]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2

[*.gradle]
indent_style = space
indent_size = 2

[*.properties]
indent_style = space
indent_size = 2

[*.sql]
indent_style = space
indent_size = 2

